[f_ksnRating]
ret = "<b>" + @ + "%</b>"

[f_Period]
ret = var l_delta; l_delta = utctime() - @; \
if( dategroup(@, false) == 0, "<b>hoje.</b>", \
if( dategroup(@, false) == -1, "<b>ontem.</b>", \
if( l_delta < 3600*24*7, "<b>há menos de uma semana</b> atrás.", \
if( l_delta < 3600*24*14, "<b>há menos de duas semanas</b> atrás.",\
if( l_delta < 3600*24*30, "<b>há menos de um mês</b> atrás.",\
if( l_delta < 3600*24*183, "<b>há mais de um mês</b> atrás.",\
if( l_delta < 3600*24*365, "<b>há mais de meio</b> ano.", "<b>há mais de um</b> ano.")))))))

