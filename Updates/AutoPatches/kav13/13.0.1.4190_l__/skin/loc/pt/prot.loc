+ "\n Localização:\t\t" + sName \
+ if(0 != tExecuted, "\n Hora de execução:\t\t" + datetime(tExecuted)) \
+ if(0 != tCreation, "\n Hora de criação:\t\t" + datetime(tCreation)) \
+ if(CreatorName, "\n Criado pelo processo:\t\t" + f_ProgramNameEx(CreatorName, CreatorPID)) \
+ if(s_IsWebSource(nCreateType), s_AppSource(nCreateType) + Source) \
+ if(2 == nCreateType && From, "\n URL recebido:\t\t" + De) \
+ if(ParentOfDriver, "\n <a onclick(dialog(\"AppHistoryWindow\", p_nPID(nPID)))>Registo de actividade da aplicação " + objfile(sName))

