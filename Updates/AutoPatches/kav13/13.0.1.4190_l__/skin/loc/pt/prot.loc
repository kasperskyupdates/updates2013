;-------------------------------- AvzTroubleshootWizard --------------------------------

[Global]
$AvzWizardErr = state == #StateFailed ? ("Falha do Assistente: " + s_ProfileError(type, lastErr))

[AvzWizard_ProgressVul]
CurObject.Name = "Objecto:"

[AvzWizard_Error]
Text        = state == #StateFailed ? "Assistente concluído com erro: " + s_AvzError(lastErr) : "Assistente concluído com sucesso"
Exit.Name   = "&Sair"
Exit.Descr  = "Conclusão do Assistente"

[s_AvzSeverity]
0   = "Corrigido"
1   = "Acções adicionais"
2   = "Acções recomendadas"
3   = "Acções vivamente recomendadas"

[AvzTroubleshootWizard]
@ = "Resolução de Problemas do Microsoft Windows"

[AvzTroubleshootWizardPages]
Welcome.Name        = "Bem-vindo ao assistente de resolução de problemas de pós-infecção do Microsoft Windows"
Search.Name         = if (Rollback, "Pesquisa de alterações", "Pesquisa de problemas")
Search.Body.Intro   = if (Rollback, "Aguarde enquanto as alterações são analisadas\n\nA análise pode demorar alguns minutos.", "Aguarde enquanto são procuradas configurações danificadas e irregulares causadas por actividades de software malicioso, falhas do sistema, ou utilitários do sistema que funcionem incorrectamente.\n\nA procura pode demorar alguns minutos.")
Problems.Name       = if (Rollback, "Pesquisa de alterações concluída", "Pesquisa de problemas concluída")
Fix.Name            = if (Rollback, "Reversão de alterações", "Eliminação de problemas")
Fix.Body.Intro      = if (Rollback, "Por favor, aguarde enquanto se revertem as alterações.", "Por favor, aguarde enquanto se restauram as configurações do sistema danificadas.")
Finish.Name         = $AvzWizardErr : if (Rollback, (ScriptMode == 5 ? "Reversão de alterações concluída com sucesso" : "Não existem alterações disponíveis para reversão"), (ScriptMode == 2 ? "Microsoft Windows restaurado com sucesso" : "Não foram detectados problemas"))

[AvzTroubleshootWizard_Welcome]
StartDescr    = "O assistente procura configurações danificadas e irregulares causadas por actividades de software malicioso, falhas do sistema e utilitários do sistema a funcionar incorrectamente."
Start         = "Procurar problemas causados por actividades de software malicioso"
Rollback      = "Reverter alterações"

[AvzTroubleshootWizard_Problems]
Desc      = if (Rollback, "Seleccione as alterações a reverter:", "Seleccione os problemas a corrigir:")
Fix.Name    = if (Rollback, "Reverter", "Corrigir")
Fix.Descr   = if (Rollback, "Reversão das alterações seleccionadas", "Restauro das configurações iniciais")

[AvzTroubleshootWizard_Problems_List]
Trouble = "Problema"
SideEff = "Consequência"

[AvzTroubleshootWizard_Finish]
Intro           =  "A resolução de problemas de pós-infecção do Microsoft Windows está concluída. Em caso de problemas, visite o Fórum de Suporte ou contacte o Suporte Técnico."
Reboot     = "Reiniciar o computador"

;-------------------------------- AvzPrivacyCleanerWizard --------------------------------

[AvzPrivacyCleanerWizard]
@ = "Eliminar vestígio de actividade"

[AvzPrivacyCleanerWizardPages]
Welcome.Name        = "Bem-vindo ao Assistente de Limpeza de vestígio de actividade"
Search.Name         = if (Rollback, "Pesquisa de alterações", "Pesquisa de sinais de actividades")
Search.Body.Intro   = if (Rollback, "Por favor, aguarde enquanto se analisam as alterações.\n\nA análise pode demorar alguns minutos.", "Por favor, aguarde enquanto se procuram listas de comandos recentemente executados e ficheiros abertos, cookies, memórias temporárias, registos e outros dados que evidenciam actividades do utilizador no computador.\n\nA procura pode demorar alguns minutos.")
Problems.Name       = if (Rollback, "Pesquisa de alterações concluída", "Pesquisa de sinais de actividades concluída")
Fix.Name            = if (Rollback, "Reversão de alterações", "Limpeza de vestígios de actividades")
Fix.Body.Intro      = if (Rollback, "Aguarde enquanto as alterações são revertidas.", "Aguarde enquanto os vestígios de actividades do utilizador são limpos.")
Finish.Name         = $AvzWizardErr : if (Rollback, (ScriptMode == 5 ? "Reversão de alterações concluída com sucesso" : "Não existem alterações disponíveis para reverter"), (ScriptMode == 2 ? "Limpeza de vestígios de actividade concluída com sucesso" : "Não foram detectados vestígios de actividades do utilizador"))

[AvzPrivacyCleanerWizard_Welcome]
Intro			= "O Assistente Eliminação de vestígios de actividade limpa do seu computador o histórico de comandos recentes e ficheiros acedidos, cookies, a memória temporária, registos e outros dados, mantendo a privacidade das suas actividades de utilizador. Terá a opção de seleccionar os dados que pretende limpar.\n\nNote que a remoção de alguns destes dados pode causar alguns inconvenientes.  Por exemplo, a remoção de cookies irá repor as configurações guardadas pelos sites que visitou."
Important               = "Feche o seu navegador de Internet antes de iniciar o diagnóstico."
Rollback		= "Reverter alterações"
Start		=  "Procurar rastreios de actividades de utilizador"

[AvzPrivacyCleanerWizard_Problems]
Desc        = if (Rollback, "Seleccione as acções a reverter:", "Seleccione os vestígios de actividade a limpar:")
Fix.Name    = if (Rollback, "Reverter", "Limpar")
Fix.Descr   = if (Rollback, "Reversão das alterações seleccionadas", "Eliminação dos problemas seleccionados")

[AvzPrivacyCleanerWizard_Problems_List]
Trouble = "Problema"
SideEff = "Consequência"

[AvzPrivacyCleanerWizard_Finish]
Intro           =  "Assistente Eliminação de vestígios de actividade concluído. Em caso de problemas, visite o Fórum de Suporte ou contacte o Suporte Técnico."
RunOnAppExit = "Limpar vestígios de actividades sempre que sair do " + $ProductName + ""
Reboot	 = "Reiniciar o computador"

