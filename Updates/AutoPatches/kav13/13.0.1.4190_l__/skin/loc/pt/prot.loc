"\n Localização:\t\t" + ObjectName + \
if(tCreation != 0, "\n Hora de criação:\t\t" + datetime(tCreation)) + \
if(ParentName,"\n Criado pelo processo:\t" + f_ProgramNameEx(ParentName,ParentPid)) + \
if(CreatorName, if($IsDriverInstall,"\n Controlador criado pelo processo:\t", "\n Criado pelo processo:\t") + f_ProgramNameEx(CreatorName,CreatorPid)) + \
