if(nPID && !($IsDriverInstall), "\n<a onclick(dialog(\"AppHistoryWindow\", p_nPID(nPID)))>Registo de actividade da aplicação</a>") + \
"\n\n" + \
if($IsDriverInstall, "\nSequência de execução da aplicação que criou o controlador:",  "\nSequência de execução da aplicação:") + \
"\nExecutado por " + Parents.format("f_appParenItem()", " > ") + \
"\n\n\n" + \
vCreators.format("f_AppParentageFormat()","\n\n")

[f_AppParentageFormat]
