;-------------------------------- AvzTroubleshootWizard --------------------------------

[Global]
$AvzWizardErr = state == #StateFailed ? ("Falha do assistente: " + s_ProfileError(type, lastErr))

[AvzWizard_ProgressVul]
CurObject.Name = "Objeto:"

[AvzWizard_Error]
Text        = state == #StateFailed ? "O assistente foi concluído com erro: " + s_AvzError(lastErr) : "O assistente concluiu com êxito"
Exit.Name   = "&Sair"
Exit.Descr  = "Conclusão do assistente"

[s_AvzSeverity]
0   = "Corrigido"
1   = "Ações adicionais"
2   = "Ações recomendadas"
3   = "Ações altamente recomendadas"

[AvzTroubleshootWizard]
@ = "Solução de Problemas do Microsoft Windows"

[AvzTroubleshootWizardPages]
Welcome.Name        = "Bem-vindo ao assistente de Solução de Problemas pós-infecção do Microsoft Windows"
Search.Name         = if (Rollback, "Pesquisa de alterações", "Pesquisa de problemas")
Search.Body.Intro   = if (Rollback, "Aguarde a análise das alterações\n\nA análise pode levar alguns minutos.", "Aguarde a pesquisa de configurações danificadas e irregulares causadas por atividade de malware, falhas do sistema ou otimizadores do sistema com funcionamento incorreto.\n\nA pesquisa pode levar alguns minutos.")
Problems.Name       = if (Rollback, "Pesquisa de alterações concluída", "Pesquisa de problemas concluída")
Fix.Name            = if (Rollback, "Reversão de alterações", "Eliminação de problemas")
Fix.Body.Intro      = if (Rollback, "Aguarde a execução da reversão de alterações.", "Aguarde a restauração das configurações do sistema danificadas.")
Finish.Name         = $AvzWizardErr : if (Rollback, (ScriptMode == 5 ? "Reversão de alterações concluída com êxito" : "Nenhuma alteração disponível para reverter"), (ScriptMode == 2 ? "Microsoft Windows restaurado com êxito" : "Nenhum problema detectado"))

[AvzTroubleshootWizard_Welcome]
StartDescr    = "O assistente pesquisa configurações danificadas e irregulares causadas por atividade de malware, falhas do sistema e otimizadores do sistema com funcionamento incorreto."
Start         = "Pesquisar problemas causados por atividade de malware"
Rollback      = "Reverter alterações"

[AvzTroubleshootWizard_Problems]
Desc      = if (Rollback, "Selecionar alterações que devem ser revertidas:", "Selecionar problemas que devem ser corrigidos:")
Fix.Name    = if (Rollback, "Reverter", "Corrigir")
Fix.Descr   = if (Rollback, "Reversão das alterações selecionadas", "Restaurando as configurações iniciais")

[AvzTroubleshootWizard_Problems_List]
Trouble = "Problema"
SideEff = "Consequência"

[AvzTroubleshootWizard_Finish]
Intro           =  "Solução de problemas pós-infecção do Microsoft Windows concluída. Se houver problemas, visite nosso Fórum de Suporte ou entre em contato com o Suporte Técnico."
Reboot     = "Reiniciar o computador"

;-------------------------------- AvzPrivacyCleanerWizard --------------------------------

[AvzPrivacyCleanerWizard]
@ = "Limpeza de Dados Particulares"

[AvzPrivacyCleanerWizardPages]
Welcome.Name        = "Bem-vindo ao Assistente de Limpeza de Dados Particulares"
Search.Name         = if (Rollback, "Pesquisa de alterações", "Pesquisa de sinais de atividade")
Search.Body.Intro   = if (Rollback, "Aguarde a análise das alterações.\n\nA análise pode levar alguns minutos.", "Aguarde a pesquisa das listas de comandos executados e de arquivos, cookies, caches, logs e outros abertos recentemente que comprovam a atividade do usuário no computador.\n\nA pesquisa pode levar alguns minutos.")
Problems.Name       = if (Rollback, "Pesquisa de alterações concluída", "Pesquisa de sinais de atividade concluída")
Fix.Name            = if (Rollback, "Reversão de alterações", "Limpeza de rastros de atividades")
Fix.Body.Intro      = if (Rollback, "Aguarde a execução da reversão de alterações.", "Aguarde a limpeza dos rastros de atividades do usuário.")
Finish.Name         = $AvzWizardErr : if (Rollback, (ScriptMode == 5 ? "Reversão de alterações concluída com êxito" : "Nenhuma alteração disponível para reversão"), (ScriptMode == 2 ? "Rastros de atividades limpos com êxito" : "Nenhum rastro de atividade detectado"))

[AvzPrivacyCleanerWizard_Welcome]
Intro			= "O Assistente de limpeza de dados particulares limpa o histórico de comandos e arquivos, cookies, caches, logs e outros dados acessados recentemente do seu computador, mantendo a privacidade das atividades do usuário. Você poderá selecionar os dados que deseja limpar.\n\nA remoção de alguns desses dados pode causar inconvenientes. Por exemplo, a remoção de cookies redefinirá as configurações salvas pelos sites visitados."
Important               = "Feche o navegador da Web antes de iniciar o diagnóstico."
Rollback		= "Reverter alterações"
Start		=  "Pesquisar rastros de atividades do usuário"

[AvzPrivacyCleanerWizard_Problems]
Desc        = if (Rollback, "Selecionar ações que devem ser revertidas:", "Selecionar rastros de atividades que devem ser limpos:")
Fix.Name    = if (Rollback, "Reverter", "Limpar")
Fix.Descr   = if (Rollback, "Reversão de alterações selecionadas", "Eliminação de problemas selecionados")

[AvzPrivacyCleanerWizard_Problems_List]
Trouble = "Problema"
SideEff = "Consequência"

[AvzPrivacyCleanerWizard_Finish]
Intro           =  "Assistente de limpeza de dados particulares concluído. Caso ocorram problemas, visite nosso Fórum de Suporte ou entre em contato com o Suporte Técnico."
RunOnAppExit = "Limpar rastros de atividades sempre ao sair do " + $ProductName
Reboot	 = "Reiniciar o computador"

